<link rel="apple-touch-icon-precomposed" sizes="120x120" href="<%= image_path("Favicon/apple-touch-icon-120x120.png") %>"/>
<link rel="apple-touch-icon-precomposed" sizes="144x144" href="<%= image_path("Favicon/apple-touch-icon-144x144.png") %>"/>
<link rel="apple-touch-icon-precomposed" sizes="152x152" href="<%= image_path("Favicon/apple-touch-icon-152x152.png") %>"/>

